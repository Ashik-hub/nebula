{
  "name": "@botbind/nebula",
  "version": "0.0.0",
  "description": "A lifecycle based Discord framework, completely modular and customisable!",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botbind/nebula.git"
  },
  "author": "BotBind's Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botbind/nebula/issues"
  },
  "homepage": "https://github.com/botbind/nebula#readme",
  "scripts": {
    "build": "cross-env NODE_ENV=production rimraf dist && babel --extensions \".ts\" src -d dist/src",
    "test": "cross-env NODE_ENV=development nodemon -e ts --exec babel-node --extensions \".ts\" src/__tests__",
    "lint": "eslint . --cache --report-unused-disable-directives && echo \"No errors found\"",
    "lint:fix": "eslint . --cache --fix && echo \"No errors found\"",
    "types:check": "tsc"
  },
  "peerDependencies": {
    "discord.js": "latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.7.5",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "discord.js": "^11.5.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/lodash.merge": "^4.6.6",
    "colors": "^1.4.0",
    "dayjs": "^1.8.16",
    "dotenv": "^8.1.0",
    "lodash.merge": "^4.6.2"
  }
}
